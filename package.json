{
  "name": "apidoc-cli",
  "version": "0.1.0",
  "description": "CLI for http://apidoc.me",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha test-lib",
    "watch": "nodemon --exec \"npm run test\" --watch src --watch test-src",
    "format": "eslint --fix src test",
    "lint": "eslint src test",
    "check": "npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kalmanb/apidoc-cli.git"
  },
  "keywords": [
    "apidoc"
  ],
  "author": "kalmanb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kalmanb/apidoc-cli/issues"
  },
  "homepage": "https://github.com/kalmanb/apidoc-cli#readme",
  "bin": {
    "apidoc": "./lib/index.js",
    "apidoc-update": "./lib/apidoc-update.js"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "lodash": "^4.12.0",
    "properties-reader": "0.0.15",
    "read-yaml": "^1.0.0",
    "superagent": "^1.8.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.2",
    "sinon": "^1.17.4"
  }
}
